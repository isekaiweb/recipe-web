{
  "extends": ["airbnb-base", "prettier", "plugin:node/recommended"],
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": [
      "error",
      {
        "endOfLine": "auto"
      }
    ],
    "no-shadow": "warn",
    "no-console": "off",
    "node/no-unsupported-features/es-syntax": [
      "error",
      {
        "version": ">=8.0.0",
        "ignores": ["restSpreadProperties"]
      }
    ]
  }
}
